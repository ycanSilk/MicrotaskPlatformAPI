{
  "baseUrl": "http://localhost:8888",
  "api": {
    "auth": {
      "login": {
        "path": "/api/users/login",
        "method": "POST",
        "requiredParams": ["username", "password"],
        "optionalParams": [],
        "responseFormat": {
          "code": "number",
          "message": "string",
          "data": {
            "token": "string",
            "tokenType": "string",
            "expiresIn": "number",
            "userInfo": {
              "id": "number",
              "username": "string",
              "email": "string|null",
              "phone": "string|null",
              "avatarUrl": "string|null"
            }
          },
          "timestamp": "number"
        },
        "description": "用户登录API，验证用户凭据并返回认证令牌"
      },
      "register": {
        "path": "/api/users/register",
        "method": "POST",
        "requiredParams": ["username", "password"],
        "optionalParams": ["email", "phone"],
        "responseFormat": {
          "code": "number",
          "message": "string",
          "data": "any",
          "timestamp": "number"
        },
        "description": "用户注册API，创建新用户账号"
      }
    }
  },
  "cors": {
    "allowOrigin": "*",
    "allowMethods": ["POST", "OPTIONS"],
    "allowHeaders": ["Content-Type", "Accept"],
    "maxAge": "86400"
  },
  "version": "1.0.0",
  "lastUpdated": "2024-08-23"
}