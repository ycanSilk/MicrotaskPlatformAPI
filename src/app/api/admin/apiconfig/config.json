{
  "baseUrl": "http://localhost:8083/api",
  "timeout": 10000,
  "headers": {
    "accept": "*/*",
    "Content-Type": "application/json"
  },
  "endpoints": {
    "auth": {
      "register": "/sys/user/register",
      "login": "/sys/user/login",
      "logout": "/sys/user/logout",
      "resetpwd": "/sys/user/reset-password",
      "refreshToken": "/sys/user/refresh",
      "userlist": "/sys/user/list",
      "forgotpwd": "/sys/user/forgot-password",
      "changepwd": "/sys/user/change-password",
      "userprofile": "/sys/user/profile",
      "createuser": "/sys/user"
    },
    "role": {
      "rolelist": "/sys/role/list",
      "createrole": "/sys/role",
      "roleassignmenus": "/sys/role/{roleId}/assign-menus",
      "getrolemenus": "/sys/role/{roleId}/menus",
      "enablerole": "/sys/role/{roleId}/enable",
      "disablerole": "/sys/role/{roleId}/disable",
      "getroledetails": "/sys/role/{roleId}",
      "updaterole": "/sys/role/{roleId}",
      "deleterole": "/sys/role/{roleId}",
      "getallroles": "/sys/role/all"
    }
  },
  "mockMode": false,
  "retryCount": 3,
  "retryDelay": 1000
}