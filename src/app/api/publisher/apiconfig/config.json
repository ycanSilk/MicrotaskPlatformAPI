{
  "baseUrl": "http://localhost:8083/api",
  "timeout": 10000,
  "headers": {
    "accept": "*/*",
    "Content-Type": "application/json"
  },
  "endpoints": {
    "auth": {
      "register": "/biz/user/register",
      "login": "/biz/user/login",
      "logout": "/biz/user/logout",
      "resetpwd": "/biz/user/reset-password",
      "refreshToken": "/biz/user/refresh",
      "userlist": "/biz/user/list"
    },
    "user": {
      "getuserinfo": "/biz/user/profile",
      "changepwd": "/biz/user/change-password"
    },
    "wallet": {
      "walletinfo": "/wallet/info",
      "topup": "/biz/wallet/topup",
      "withdraw": "/biz/wallet/withdraw"
    }
  },
  "mockMode": false,
  "retryCount": 3,
  "retryDelay": 1000
}