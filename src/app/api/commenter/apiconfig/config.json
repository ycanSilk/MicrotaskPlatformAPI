{
  "baseUrl": "http://localhost:8080/api/ind",
  "timeout": 5000,
  "headers": {
    "accept": "*/*",
    "Content-Type": "application/json"
  },
  "endpoints": {
    "auth": {
      "register": "/user/register",
      "login": "/user/login",
      "logout": "/user/logout",
      "resetpwd": "/user/reset-password",
      "refreshToken": "/user/refresh",
      "userlist": "/user/list",
      "forgotpwd": "/user/forgot-password",
      "changepwd": "/user/change-password",
      "userprofile": "/user/profile"
    },
    "bankcards": {
      "banklist": "/bank/list",
      "bankdetail": "/bank/detail"
    }
  },
  "mockMode": false,
  "retryCount": 3,
  "retryDelay": 1000
}